pipeline {
  agent any
  
  stages {

        stage('Download') {
            steps {
                sh '''
				      
                    bash ./simple-java-maven-app/jenkins/build/deploy.bash
                '''
            }

        }
  


     stage('Rebuild') {
            steps {
                sh ' bash ./simple-java-maven-app/jenkins/build/createimage.bash'
            }

        }

}
}