agentName = "slave"
agentLabel = "${-> println 'Right Now the Agent Name is ' + agentName; return agentName}"

pipeline {
  
  stages {

        stage('Download') {
		    node ( agentLabel as String ){
            steps {
                sh '''
				      
                    bash ./simple-java-maven-app/jenkins/build/deploy.bash
                '''
            }

        }
  
}
}

}